{
  "name": "@browserbasehq/stagehand",
  "version": "1.12.0",
  "description": "An AI web browsing framework focused on simplicity and extensibility.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "2048": "yarn run build-dom-scripts && tsx examples/2048.ts",
    "popup": "yarn run build-dom-scripts && tsx examples/popup.ts",
    "example": "yarn run build-dom-scripts && tsx examples/example.ts",
    "langchain": "yarn run build-dom-scripts && tsx examples/langchain.ts",
    "debug-url": "yarn run build-dom-scripts && tsx examples/debugUrl.ts",
    "external-client": "yarn run build-dom-scripts && tsx examples/external_client.ts",
    "instructions": "yarn run build-dom-scripts && tsx examples/instructions.ts",
    "ai-sdk-client": "yarn run build-dom-scripts && tsx examples/ai_sdk_example.ts",
    "actionable_observe_example": "yarn run build-dom-scripts && tsx examples/actionable_observe_example.ts",
    "format": "prettier --write .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint .",
    "cache:clear": "rm -rf .cache",
    "evals": "yarn run build-dom-scripts && tsx evals/index.eval.ts",
    "e2e": "yarn run build-dom-scripts && cd evals/deterministic && yarn playwright test --config=e2e.playwright.config.ts",
    "e2e:bb": "yarn run build-dom-scripts && cd evals/deterministic && yarn playwright test --config=bb.playwright.config.ts",
    "build-dom-scripts": "tsx lib/dom/genDomScripts.ts",
    "build-types": "tsc --emitDeclarationOnly --outDir dist",
    "build-js": "tsup lib/index.ts --dts",
    "build": "yarn run lint && yarn run build-dom-scripts && yarn run build-js && yarn run build-types",
    "lint": "yarn run prettier && yarn run eslint",
    "release": "yarn run build && changeset publish",
    "release-canary": "yarn run build && changeset version --snapshot && changeset publish --tag alpha",
    "dev": "yarn dlx nodemon --watch lib --ignore lib/dom/build --ext ts,tsx --exec \"yarn run build\"",
    "postinstall": "yarn run build"
  },
  "files": [
    "dist/**",
    "lib/**"
  ],
  "keywords": [],
  "author": "Browserbase",
  "license": "MIT",
  "devDependencies": {
    "@ai-sdk/google": "1.1.14",
    "@ai-sdk/openai": "1.1.12",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.16.0",
    "@langchain/core": "^0.3.40",
    "@langchain/openai": "^0.4.4",
    "@playwright/test": "^1.42.1",
    "@types/adm-zip": "^0.5.7",
    "@types/cheerio": "^0.22.35",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.13",
    "adm-zip": "^0.5.16",
    "ai": "4.1.41",
    "autoevals": "^0.0.64",
    "braintrust": "^0.0.171",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "chromium-bidi": "0.10.2",
    "deepmerge": "4.3.1",
    "dotenv": "16.1.4",
    "esbuild": "0.24.2",
    "eslint": "^9.16.0",
    "express": "^4.21.0",
    "globals": "^15.13.0",
    "multer": "^1.4.5-lts.1",
    "openai": "4.24.1",
    "prettier": "^3.2.5",
    "string-comparison": "^1.3.0",
    "tsup": "^8.2.1",
    "tsx": "4.7.3",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.17.0",
    "zod": "3.22.4"
  },
  "peerDependencies": {
    "@playwright/test": "^1.42.1",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "openai": "^4.62.1",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@browserbasehq/sdk": "^2.0.0",
    "ws": "^8.18.0",
    "zod-to-json-schema": "^3.23.5"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browserbase/stagehand.git"
  },
  "bugs": {
    "url": "https://github.com/browserbase/stagehand/issues"
  },
  "homepage": "https://github.com/browserbase/stagehand#readme"
}
